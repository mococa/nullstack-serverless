{
  "name": "nullstack-serverless",
  "version": "0.0.1",
  "description": "Nullstack construct for CDK",
  "author": "mococa <luizfelipesmoureau@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/mococa/nullstack-serverless",
  "repository": {
    "url": "https://github.com/mococa/nullstack-serverless.git"
  },
  "keywords": [
    "nullstack",
    "cdk",
    "serverless",
    "lambda",
    "s3",
    "bucket",
    "ssr",
    "ssg",
    "spa"
  ],
  "main": "dist/src",
  "module": "dist/src",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "prebootstrap": "yarn synth",
    "bootstrap": "cdk bootstrap",
    "predeploy": "yarn bootstrap",
    "deploy": "cdk deploy --require-approval never",
    "destroy": "cdk destroy --force true",
    "prereup": "yarn destroy",
    "reup": "yarn deploy"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^14.14.37",
    "aws-cdk": "2.82.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.4.2"
  },
  "peerDependencies": {
    "aws-cdk": "2.82.0",
    "aws-cdk-lib": "2.82.0",
    "constructs": "^10.0.0"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.10.4",
    "adm-zip": "^0.5.10",
    "aws-cdk-lib": "2.82.0",
    "aws-serverless-express": "^3.4.0",
    "express": "^4.18.2",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
